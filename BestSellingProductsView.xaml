<CollectionView ItemsSource="{Binding BoughtProducts}">
    <CollectionView.ItemTemplate>
        <DataTemplate>
            <StackLayout>
                <Label Text="{Binding Client.Name}" />
                <Label Text="{Binding GroceryList.Name}" />
            </StackLayout>
        </DataTemplate>
    </CollectionView.ItemTemplate>
</CollectionView>